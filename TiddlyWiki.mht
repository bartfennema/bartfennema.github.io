<PRE>{=0A=
    "tiddlers": {=0A=
        "$:/plugins/ahahn/tinka/createDialog": {=0A=
            "created": "20150429173743678",=0A=
            "text": "\\define =
plugin-tiddler-selection()\n[[$(target)$]plugintiddlers[]]\n\\end\n\n\\de=
fine =
qualified-modify-add()\n$(qualifiedTiddler)$-$(target)$-add\n\\end\n\n\\d=
efine =
qualified-modify-diff()\n$(qualifiedTiddler)$-$(target)$-diff\n\\end\n\n\=
\define =
