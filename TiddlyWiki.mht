\\end\n\n\\define =
added-filter()\n[[$(addedTiddlers)$]tags[]]\n\\end\n\n\\define =
concatPluginTitle(prefix, =
sep)\n$prefix$$(createTitleOrg)$$sep$$(createTitleName)$\n\\end\n\n\\defi=
ne tiddlerReference(ref)\n$(currentTiddler)$$ref$\n\\end\n\n\\define =
varsReference(ref)\n$(TinkaVars)$$ref$\n\\end\n\n\n!! Create New =
Plugin\n\n&lt;span class=3D\"tinka-note\"&gt;Usage: Enter the necessary =
metadata for your plugin and use the Filter selection below to pick the =
tiddlers that should be added to the plugin. After selecting the =
tiddlers, press 'Package Plugin'. Refer to the =
[[Documentation|$:/plugins/ahahn/tinka/usage]] for further =
help.&lt;/span&gt;\n\n&lt;$set name=3D\"target\" =
value=3D\"skeleton\"&gt;\n&lt;$set name=3D\"qualifiedTiddler\" =
value=3D&lt;&lt;qualify \"$:/temp/tinka/modify\"&gt;&gt; &gt;\n\n!!! =
Step 1: Enter Metadata\n\n\n&lt;$set name=3D\"currentTiddler\" =
value=3D&lt;&lt;qualified-modify-diff&gt;&gt;&gt;\n\t&lt;$transclude =
mode=3D\"block\" =
tiddler=3D\"$:/plugins/ahahn/tinka/createMetadata\"/&gt;\n&lt;/$set&gt;\n=
\n!!! Step 2: Add Tiddlers\n\nUse the search box below to select the =
tiddlers you want to add to the =
plugin.\n\n{{$:/plugins/ahahn/tinka/searchDisplay}}\n\n\n''Added =
Tiddlers''\n\n&lt;$set name=3D\"addedTiddlers\" =
value=3D&lt;&lt;qualified-modify-add&gt;&gt; &gt;\n&lt;ul&gt;\n&lt;$list =
filter=3D&lt;&lt;added-filter&gt;&gt; emptyMessage=3D\"&lt;i&gt;No =
tiddlers added.&lt;/i&gt;\"&gt;\n\t\t&lt;li =
class=3D\"tinka-list-item\"&gt;\n        &lt;$checkbox =
tiddler=3D&lt;&lt;qualified-modify-add&gt;&gt; tag=3D{{!!title}} /&gt;\n =
       &lt;$link to=3D{{!!title}}&gt;{{!!title}}&lt;/$link&gt;\n        =
&lt;/li&gt;\n&lt;/$list&gt;\n&lt;/ul&gt;\n&lt;/$set&gt;\n\n\n&lt;$button =
class=3D\"tc-btn-big-green tinka-orange\"&gt;\n&lt;$set =
name=3D\"TinkaVars\" value=3D\"$:/temp/tinka/CreateVars\"&gt;\n&lt;$set =
name=3D\"currentTiddler\" =
value=3D&lt;&lt;qualified-modify-diff&gt;&gt;&gt;\n\t&lt;$action-deleteti=
ddler $tiddler=3D&lt;&lt;TinkaVars&gt;&gt; /&gt;\n\t&lt;$action-setfield =
$tiddler=3D&lt;&lt;TinkaVars&gt;&gt; result=3D\"false\" =
/&gt;\n\n\t&lt;!-- Check whether a plugin title was given =
--&gt;\n\t&lt;$tinka-check text=3D{{!!create-title-org}} =
pattern=3D\"^[^\\s]+$\"&gt;\n\t\t&lt;$tinka-filter =
verb=3D\"fail\"&gt;\n\t\t\t&lt;$action-setfield =
$tiddler=3D&lt;&lt;TinkaVars&gt;&gt; =
result=3D\"true\"/&gt;\n\t\t\t&lt;$action-setfield =
$tiddler=3D&lt;&lt;TinkaVars&gt;&gt; errorTitle=3D\"The plugin title is =
not allowed to be empty or contain =
whitespaces.\"/&gt;\n\t\t&lt;/$tinka-filter&gt;\n\t&lt;/$tinka-check&gt;\=
n\t\t\n\t&lt;$tinka-check text=3D{{!!create-title-name}} =
